{
  "name": "site-boilerplate",
  "version": "0.1.0",
  "private": true,
  "author": "Logan Thirion",
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "bfj": "^7.0.2",
    "bookshelf": "^1.2.0",
    "camelcase": "^6.1.0",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "connect-session-knex": "^2.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "css-loader": "5.0.1",
    "csurf": "^1.11.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.3",
    "express-session": "^1.17.1",
    "file-loader": "6.2.0",
    "fs-extra": "^9.0.1",
    "helmet": "^4.3.1",
    "html-webpack-plugin": "4.5.1",
    "identity-obj-proxy": "3.0.0",
    "knex": "^0.21.15",
    "mini-css-extract-plugin": "1.3.4",
    "nunjucks": "^3.2.2",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "9.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "5.0.2",
    "prettier": "^2.2.1",
    "prompts": "2.4.0",
    "react": "^17.0.1",
    "react-app-polyfill": "^2.0.0",
    "react-dev-utils": "^11.0.1",
    "react-dom": "^17.0.1",
    "react-refresh": "^0.9.0",
    "react-router-dom": "^5.2.0",
    "resolve": "1.19.0",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "10.1.1",
    "semver": "7.3.4",
    "sqlite3": "^5.0.0",
    "style-loader": "2.0.0",
    "superagent": "^6.1.0",
    "terser-webpack-plugin": "4.2.3",
    "ts-pnp": "1.2.0",
    "url-loader": "4.1.1",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "server": "node server/server.js",
    "preserver": "npm run rollback && npm run migrate && npm run seed",
    "test": "npm run test:unit",
    "test:unit": "node scripts/test.js",
    "test:mutate": "npx stryker run",
    "migrate": "npx knex migrate:latest --knexfile ./server/knexfile.js",
    "rollback": "npx knex migrate:rollback --knexfile ./server/knexfile.js",
    "seed": "npx knex seed:run --knexfile ./server/knexfile.js",
    "format": "npx prettier --write .",
    "lint": "npx eslint . --fix --format table",
    "hooks": "sh ./hooks/setup-hooks.sh",
    "mutate": "npm run test:mutate"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/server"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "server/**/*.{js,jsx,ts,tsx}",
      "!server/**/*.d.ts",
      "!src/client/*.js",
      "!server/server.js",
      "!server/knexfile.js",
      "!server/data/db.js",
      "!src/reportWebVitals.js",
      "!src/index.js",
      "!src/mocks/*.js",
      "!**/testHelpers.js"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}",
      "<rootDir>/server/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/server/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "./node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@stryker-mutator/core": "^5.2.2",
    "@stryker-mutator/jest-runner": "^5.2.2",
    "@svgr/webpack": "5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.0",
    "babel-loader": "8.2.2",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^8.21.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.9.2",
    "eslint-webpack-plugin": "^2.1.0",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "jest-resolve": "26.6.2",
    "jest-watch-typeahead": "0.6.1",
    "msw": "^0.25.0",
    "supertest": "^6.0.1",
    "supertest-session": "^4.1.0",
    "webpack": "4.46.0",
    "webpack-dev-server": "3.11.2",
    "webpack-manifest-plugin": "3.0.0",
    "workbox-webpack-plugin": "6.0.2"
  }
}
